{
  "manifest_version": 3,
  "version": "1.0.11",
  "name": "Silent Web",
  "description": "Tools for remove web content",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "options_page": "options.html",
  "action": {
    "default_title": "A-React-Title",
    "default_popup": "popup.html"
  },
  "permissions": ["storage", "tabs"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.reddit.com/*"],
      "js": ["reddit.js"],
      "css": ["style.css"],
      "all_frames": true
    },
    {
      "matches": ["*://*.dr.dk/*"],
      "js": ["dr.dk.js"],
      "css": ["style.css"]
    },
    {
      "matches": [
        "*://*.tv2.dk/*",
        "*://*.nyheder.tv2.dk/*",
        "*://*.sport.tv2.dk/*",
        "*://*.vejr.tv2.dk/*",
        "*://*.tv.tv2.dk/*"
      ],
      "js": ["tv2.dk.js"],
      "css": ["style.css"]
    },
    {
      "matches": ["*://ekstrabladet.dk/*"],
      "js": ["ekstrabladet.dk.js"],
      "css": ["style.css"]
    },
    {
      "matches": ["*://politiken.dk/*"],
      "js": ["politiken.dk.js"],
      "css": ["style.css"]
    },
    {
      "matches": ["*://*.bt.dk/*"],
      "js": ["bt.dk.js"],
      "css": ["style.css"]
    },
    {
      "matches": ["*://*.jp.dk/*", "*://*.jyllands-posten.dk/*"],
      "js": ["jp.dk.js"],
      "css": ["style.css"]
    },
    {
      "matches": ["*://*.berlingske.dk/*"],
      "js": ["berlingske.dk.js"],
      "css": ["style.css"]
    },
    {
      "matches": ["*://*.msn.dk/*", "*://*.msn.com/*"],
      "js": ["msn.dk.js"],
      "css": ["style.css"]
    },
    {
      "matches": ["*://*.finans.dk/*"],
      "js": ["finans.dk.js"],
      "css": ["style.css"]
    },
    {
      "matches": ["*://*.dagens.dk/*", "*://*.dagens.com/*"],
      "js": ["dagens.dk.js"],
      "css": ["style.css"]
    },
    {
      "matches": ["*://*.seoghoer.dk/*"],
      "js": ["seoghoer.dk.js"],
      "css": ["style.css"]
    },
    {
      "matches": ["*://*.nytimes.com/*"],
      "js": ["nytimes.com.js"],
      "css": ["style.css"]
    },
    {
      "matches": ["*://*.dinavis.dk/*", "*://*.lokalavisen.dk/*"],
      "js": ["dinavis.dk.js"],
      "css": ["style.css"]
    },
    {
      "matches": ["*://*.people.com/*"],
      "js": ["people.com.js"],
      "css": ["style.css"],
      "run_at": "document_start"
    },
    {
      "matches": ["*://*.bbc.co.uk/*", "*://*.bbc.com/*"],
      "js": ["bbc.com.js"],
      "css": ["style.css"]
    },
    {
      "matches": ["*://*.foxnews.com/*"],
      "js": ["foxnews.com.js"],
      "css": ["style.css"]
    },
    {
      "matches": ["*://*.cnn.com/", "*://*.edition.cnn.com/*"],
      "js": ["cnn.com.js"],
      "css": ["style.css"]
    },
    {
      "matches": ["*://news.google.com/*"],
      "js": ["news.google.com.js"],
      "css": ["style.css"]
    },
    {
      "matches": ["*://*.yahoo.com/*"],
      "js": ["yahoo.com.js"],
      "css": ["style.css"]
    },
    {
      "matches": ["*://*.theguardian.com/*"],
      "js": ["theguardian.com.js"],
      "css": ["style.css"]
    },
    {
      "matches": ["*://*.dailymail.co.uk/*"],
      "js": ["dailymail.co.uk.js"],
      "css": ["style.css"]
    },
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["allDomains.js"],
      "css": ["style.css"],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["*.js"],
      "matches": ["*://*/*"]
    }
  ]
}
